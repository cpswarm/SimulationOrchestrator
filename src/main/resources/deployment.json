              "image": "cpswarm/frevo-docker:1.0.3",
              "args": [
                "-n",
                "pert-demoenergy-virtus.ismb.polito.it",
                "-ip",
                "130.192.86.237",
                "-p",
                "5222",
                "-r",
                "cpswarm",
                "-cid",
                "frevo",
                "-cp",
                "blah",
                "-c",
                "/home/"
