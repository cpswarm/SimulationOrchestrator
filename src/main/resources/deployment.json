    {
      "metadata": {
        "name": "stage",
        "namespace": "default",
        "generation": 1,
        "labels": {
          "k8s-app": "stage"
        }
      },
      "spec": {
        "replicas": 1,
        "selector": {
          "matchLabels": {
            "k8s-app": "stage"
          }
